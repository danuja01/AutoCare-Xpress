{
  "expo": {
    "scheme": "acme",
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "iosAllowsAlert": true,
          "iosAllowsBadge": true,
          "iosAllowsSound": true
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "name": "CSSE-frontend",
    "slug": "CSSE-frontend",
    "ios": {
      "bundleIdentifier": "com.anonymous.CSSE-frontend"
    }
  }
}
